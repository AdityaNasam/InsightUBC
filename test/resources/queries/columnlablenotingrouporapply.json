{
	"title": "In sections dataset sections grouped by ID, find entries whose Department is \"cpsc\" and Average is greater than 80; show ID, Department and minAvg, where minAvg is the MIN of Average; sort in descending order by minAvg.",
	"input": {
		"WHERE": {
			"AND": [
				{
					"IS": {
						"sections_dept": "cpsc"
					}
				},
				{
					"GT": {
						"sections_avg": 80
					}
				}
			]
		},
		"OPTIONS": {
			"COLUMNS": [
				"sections_id",
				"sections_dept",
				"minAvg"
			],
			"ORDER": {
				"dir": "DOWN",
				"keys": [
					"minAvg"
				]
			}
		},
		"TRANSFORMATIONS": {
			"GROUP": [
				"sections_id"
			],
			"APPLY": [
				{
					"minAvg": {
						"MIN": "sections_avg"
					}
				}
			]
		}
	},
	"errorExpected": true,
	"expected": "InsightError"
}