{
	"title": "In rooms dataset rooms grouped by Short Name, find entries whose Furniture includes \"Tables\" and Seats is greater than 200; show Short Name and sumSeats, where sumSeats is the SUM of Seats; sort in descending order by sumSeats.",
	"input": {
		"WHERE": {
			"AND": [
				{
					"IS": {
						"rooms_furniture": "*Tables*"
					}
				},
				{
					"GT": {
						"rooms_seats": 200
					}
				}
			]
		},
		"OPTIONS": {
			"COLUMNS": [
				"rooms_shortname",
				"sumSeats"
			],
			"ORDER": {
				"dir": "DOWN",
				"keys": [
					"sumSeats"
				]
			}
		},
		"TRANSFORMATIONS": {
			"GROUP": [
				"rooms_shortname"
			],
			"APPLY": [
				{
					"sumSeats": {
						"SUM": "rooms_seats"
					}
				}
			]
		}
	},
	"errorExpected": false,
